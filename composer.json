{
    "name": "epicsagas/zipper",
    "type": "library",
    "description": "This is a little neat helper for the ZipArchive methods with handy functions",
    "keywords": ["laravel", "ZIP", "Archive"],
    "homepage": "http://github.com/Chumper/zipper",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Nils Plaschke",
            "email": "github@nilsplaschke.de",
            "homepage": "http://nilsplaschke.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "ext-zip": "*",
        "illuminate/support": "^7.0|^8.0",
        "illuminate/filesystem": "^7.0|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0|^9.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Epicsagas\\Zipper\\": "src/Chumper/Zipper"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Epicsagas\\Zipper\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Epicsagas\\Zipper\\ZipperServiceProvider"
            ],
            "aliases": {
                "Zipper": "Epicsagas\\Zipper\\Zipper"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
